mport mapMarkerImg from \"../Assets/img/pin_sos.svg\";\n\nexport default function Notification() {\n  const [pinArray, setPinArray] = useState([]);\n  const [ubs, setUbs] = useState(1);\n  const [notificationList, setNotificationList] = useState([]);\n\n  // ------------------------------ Api notificações\n  useEffect(() => {\n    async function fetchMyAPI() {\n      const response = await fetch(\"http://localhost:3001/ubs/DENcount\");\n      setPinArray(await response.json());\n    }\n    fetchMyAPI();\n  }, []);\n\n  // ------------------------------ Api ubs\n  useEffect(() => {\n    async function fetchMyAPI() {\n      const response = await fetch(`http://localhost:3001/den/byUBS/${ubs}`);\n      setNotificationList(await response.json());\n    }\n    fetchMyAPI();\n  }, [ubs]);\n\n  const mapIcon = Leaflet.icon({\n    iconUrl: mapMarkerImg,\n    iconSize: [38, 48],\n    iconAnchor: [29, 68],\n    popupAnchor: [170, -10],\n  });\n\n  // ------------------------------ Formatação da data de mensagens\n  const formatDate = (rawDate) => {\n    const myDate = new Date(rawDate);\n    const d = myDate.getDay();\n    const mo = myDate.getMonth() + 1;\n    const y = myDate.getFullYear();\n    // const h = myDate.getHours();\n    // const mi = myDate.getMinutes();\n\n  return `${d}/${mo}/${y}`; {/*- ${h}:${mi}*/};\n  };\n  return (\n    <div className=\"container\">\n      <div className=\"d-flex row\" style={{ height: \"90vh\" }}>\n        <div\n          className=\"card_not col-5 table-responsive p-3 mt-5 text-white\"\n          style={{ width: \"20vw\", height: \"\", background: \"#00b9b3\" }}\n        >\n          <h4 className=\"border-bottom border-white\">NOTIFICAÇÕES RECENTES</h4>\n          {notificationList.length > 0 &&\n            notificationList.map((not) => (\n              <div className=\"m-3 row\">\n                <div className=\"col-8\">\n                  <p>{not.nomeUbs}</p>\n                  <p className=\"lead font-weight-bold \">{not.nome}</p>\n                </div>\n                <div className=\"col-4 justify-content-center flex-column py-auto d-flex align-items-center\">\n                  <p>Data da Falta:</p>\n                  <p>{formatDate(not.data_ocorrencia)}</p>\n                </div>\n              </div>\n            ))}\n        </div>\n        <div id=\"reportMap\" className=\"col-7 mt-5 rounded-3\">\n          <MapContainer\n            center={[-23.49, -46.43]}\n            zoom={13}\n            style={{ width: \"100%\", height: \"100%\" }}\n            className=\"card_map\"\n          >\n            <TileLayer url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\" />\n\n            {pinArray.map((pin) => (\n              <Marker icon={mapIcon} position={[pin.latitude, pin.longitude]}>\n                <Tooltip>{pin.qtde}</Tooltip>\n                <Popup\n                  closeButton={false}\n                  minWidth={240}\n                  maxWidth={240}\n                  className=\"map-popup\"\n                >\n                  <button onClick={() => setUbs(pin.id)}>\n                    Mostrar notificações\n                  </button>\n                  {pin.nomeUbs}\n                </Popup>\n              </Marker>\n            ))}\n          </MapContainer>\n        </div>\n\n      </div>\n        <button className=\"btn-roxo my-3 border-0\" style={{borderRadius: \"20px\", height: \"40px\", width: \"40%\"}}>Exportar todas as notificações (.CSV)</button>\n    </div>\n  );\n}\n","/home/nayara/projects/htdocs/ProjetoSaude/SPRINT_03/frontend/src/Components/CardReport/index.jsx",[],{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":13,"column":15,"nodeType":"64","endLine":17,"endColumn":17},{"ruleId":"65","severity":1,"message":"66","line":2,"column":10,"nodeType":"67","messageId":"68","endLine":2,"endColumn":14},{"ruleId":"69","severity":1,"message":"70","line":3,"column":1,"nodeType":"71","endLine":